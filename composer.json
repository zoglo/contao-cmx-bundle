{
    "name": "kiwi/contao-cmx",
    "keywords": ["contao", "cmx", "ux", "cms", "backend"],
    "type": "contao-bundle",
    "description": "This contao bundle contains several functionalities to enhance UX within the Contao CMS.",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Katrin Zimmermann",
            "homepage": "https://github.com/diekatrin"
        },
        {
            "name": "KIWI. Werbeagentur GmbH",
            "homepage": "https://www.kiwi.de"
        }
    ],
    "require": {
        "php": "^8.3",
        "contao/core-bundle": "^5.3",
        "codefog/contao-haste": "^5.3"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4" : {
            "Kiwi\\Contao\\CmxBundle\\" : "src/"
        }
    },
    "extra":{
        "contao-manager-plugin": "Kiwi\\Contao\\CmxBundle\\ContaoManager\\Plugin"
    }
}

